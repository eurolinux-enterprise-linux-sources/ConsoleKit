From 8f22075ac4ed012e5b4cf9aa41a2d55238a7fbcd Mon Sep 17 00:00:00 2001
From: Michal Sekletar <msekleta@redhat.com>
Date: Sun, 17 Jan 2016 17:19:50 +0100
Subject: [PATCH] Don't define rpath in libck-connector.la

This in turn should fix rpmlint warning about ck-launch-session

Related: #634935
---
 libck-connector/Makefile.in | 2 +-
 1 file changed, 1 insertion(+), 1 deletion(-)

diff --git a/libck-connector/Makefile.in b/libck-connector/Makefile.in
index 455effa..66c7dd7 100644
--- a/libck-connector/Makefile.in
+++ b/libck-connector/Makefile.in
@@ -364,7 +364,7 @@ clean-libLTLIBRARIES:
 	  rm -f "$${dir}/so_locations"; \
 	done
 libck-connector.la: $(libck_connector_la_OBJECTS) $(libck_connector_la_DEPENDENCIES) 
-	$(libck_connector_la_LINK) -rpath $(libdir) $(libck_connector_la_OBJECTS) $(libck_connector_la_LIBADD) $(LIBS)
+	$(libck_connector_la_LINK) $(libck_connector_la_OBJECTS) $(libck_connector_la_LIBADD) $(LIBS)
 
 clean-noinstPROGRAMS:
 	@list='$(noinst_PROGRAMS)'; for p in $$list; do \
-- 
